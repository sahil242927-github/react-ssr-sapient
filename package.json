{
  "name": "react-ssr-project",
  "version": "1.0.0",
  "description": "React SSR project",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-client": "webpack --config webpack.client.js --mode=development --watch",
    "dev:build-server": "webpack --config webpack.server.js --mode=development --watch",
    "test": "jest",
    "start": "npm-run-all --parallel dev:*",
    "heroku-postbuild": "npm run dev && cd client && npm install --only=dev:* --no-shrinkwrap"
  },
  "author": "Mohd Sahil",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "babel": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "chart.js": "^2.9.4",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.0",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^3.1.0",
    "style-loader": "^1.2.1",
    "webpack-messages": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "compression": "^1.7.4",
    "concurrently": "^5.2.0",
    "css-loader": "^3.6.0",
    "jest": "^24.9.0",
    "node-fetch": "^2.6.0",
    "redux-mock-store": "^1.5.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  }
}
